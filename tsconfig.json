{
  "compilerOptions": {
      "forceConsistentCasingInFileNames": true,
      "module": "commonjs",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "outDir": "./build",
      "rootDir": "./src",
      "target": "es6",
      "skipLibCheck": true,
      "strict": true
  },
  "include": ["./src/**/*"]
}